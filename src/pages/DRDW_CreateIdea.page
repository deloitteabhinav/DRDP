                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-3 label-wrapper left-aligned">
                                                <label>{!$ObjectType.DRDW_Project__c.Fields.CSF_In_vitro_PoP_Planned__c.Label}</label>
                                                <apex:outputpanel rendered="{!IF($ObjectType.DRDW_Project__c.Fields.CSF_In_vitro_PoP_Planned__c.inlinehelptext != NULL,true,false)}">
