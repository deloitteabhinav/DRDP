                                            </div>
                                            <div class="col-xs-3 label-wrapper left-aligned">
                                                <label>{!$ObjectType.DRDW_Project__c.Fields.CSF_In_vitro_PoP_Actual__c.Label}</label>
                                                <apex:outputpanel rendered="{!IF($ObjectType.DRDW_Project__c.Fields.CSF_In_vitro_PoP_Actual__c.inlinehelptext != NULL,true,false)}">
                                                    <div class="helpOrb tooltip-sfdc left-aligned">
