                                                        <span>{!$ObjectType.DRDW_Project__c.Fields.LI_Assay_Availability_Actual__c.inlinehelptext}</span>
                                                    </div>          
                                                </apex:outputpanel>
                                            </div>
                                            <div class="col-xs-3 field-wrapper left-aligned">
                                                <apex:inputField value="{!oIdea.LI_Assay_Availability_Actual__c}"/>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-offset-6 col-xs-3 label-wrapper left-aligned">
                                                <label>{!$ObjectType.DRDW_Project__c.Fields.LI_Assay_Availability_Difference__c.Label}</label>
                                                <apex:outputpanel rendered="{!IF($ObjectType.DRDW_Project__c.Fields.LI_Assay_Availability_Difference__c.inlinehelptext != NULL,true,false)}">
                                                    <div class="helpOrb tooltip-sfdc left-aligned">
                                                        <span>{!$ObjectType.DRDW_Project__c.Fields.LI_Assay_Availability_Difference__c.inlinehelptext}</span>
                                                    </div>          
                                                </apex:outputpanel>
                                            </div>
                                            <div class="col-xs-3 field-wrapper left-aligned">
                                                <apex:outputField value="{!oIdea.LI_Assay_Availability_Difference__c}"/>{!if(oIdea.LI_Assay_Availability_Difference__c != null,' days','')}
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-3 label-wrapper left-aligned">
                                                <label>{!$ObjectType.DRDW_Project__c.Fields.LI_Flow_Scheme_w_Success_Criteria_Pla__c.Label}</label>
                                                <apex:outputpanel rendered="{!IF($ObjectType.DRDW_Project__c.Fields.LI_Flow_Scheme_w_Success_Criteria_Pla__c.inlinehelptext != NULL,true,false)}">
