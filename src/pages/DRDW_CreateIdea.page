                                                <apex:outputpanel rendered="{!IF($ObjectType.DRDW_Project__c.Fields.LI_Leading_Scaffold_Molecular_Id_Act__c.inlinehelptext != NULL,true,false)}">
                                                    <div class="helpOrb tooltip-sfdc left-aligned">
                                                        <span>{!$ObjectType.DRDW_Project__c.Fields.LI_Leading_Scaffold_Molecular_Id_Act__c.inlinehelptext}</span>
                                                    </div>          
                                                </apex:outputpanel>
