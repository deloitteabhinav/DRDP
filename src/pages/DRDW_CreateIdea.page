                                                <apex:outputpanel rendered="{!IF($ObjectType.DRDW_Project__c.Fields.CSF_In_vitro_PoP_Difference__c.inlinehelptext != NULL,true,false)}">
                                                    <div class="helpOrb tooltip-sfdc left-aligned">
