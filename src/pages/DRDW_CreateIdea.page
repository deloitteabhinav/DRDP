                                                <label>{!$ObjectType.DRDW_Project__c.Fields.LI_Leading_Scaffold_Molecular_Id_Plan__c.Label}</label>
                                                <apex:outputpanel rendered="{!IF($ObjectType.DRDW_Project__c.Fields.LI_Leading_Scaffold_Molecular_Id_Plan__c.inlinehelptext != NULL,true,false)}">
                                                    <div class="helpOrb tooltip-sfdc left-aligned">
                                                        <span>{!$ObjectType.DRDW_Project__c.Fields.LI_Leading_Scaffold_Molecular_Id_Plan__c.inlinehelptext}</span>
                                                    </div>          
                                                </apex:outputpanel>
                                            </div>
                                            <div class="col-xs-3 field-wrapper left-aligned">
                                                <apex:inputField value="{!oIdea.LI_Leading_Scaffold_Molecular_Id_Plan__c}"/>
                                            </div>
                                            <div class="col-xs-3 label-wrapper left-aligned">
