                                                                    <span>{!$ObjectType.DRDW_Project__c.Fields.Total_budget__c.inlinehelptext}</span>
                                                                </div>          
                                                            </apex:outputpanel>      
                                                        </div>
                                                        <apex:variable var="amount" value="{!oStudy.Total_budget__c}" />
