        function isValidSearchString(searchString) {
            var SEARCH_TRIGGER_THRESHOLD = 2;
            var trimmedSearchString = searchString.trim();

            if (trimmedSearchString.length >= SEARCH_TRIGGER_THRESHOLD) {
                return true;
            } else {
                return false;
            }
        }

