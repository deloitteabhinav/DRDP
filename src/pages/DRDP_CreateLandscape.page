            if (window.event && window.event.keyCode == 13 || ev.which == 13) {
                var searchString = (document.getElementsByClassName("keyword_DataAsset_Search_Text"))[0].value;
                if (isValidSearchString(searchString)) {
                    doSearchAf();
                }
                return false;
            } else {
                return true;
            }
        }
