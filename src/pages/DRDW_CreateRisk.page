<apex:page sidebar="false" showHeader="false" standardStylesheets="false" id="thePage" Standardcontroller="DRDW_Risk__c" extensions="DRDW_CreateRiskController">
<head>
        <title>Create Risk</title>
        <c:DRDW_QFOR_Styles />
        <apex:stylesheet value="{!URLFOR($Resource.DRDW_Chosen,'chosen.css')}"/>
        <!--<apex:stylesheet value="{!URLFOR($Resource.DRDW_Chosen,'docsupport/style.css')}"/>
        <apex:stylesheet value="{!URLFOR($Resource.DRDW_Chosen,'docsupport/prism.css')}"/>
        <apex:stylesheet value="{!URLFOR($Resource.DRDW_Chosen,'chosen.css')}"/>
        <apex:includeScript value="{!URLFOR($Resource.DRDW_Chosen,'chosen.jquery.js')}" />
        <!--<apex:includeScript value="{!'https://harvesthq.github.io/chosen/chosen.jquery.js'}" />
        <apex:includeScript value="{!URLFOR($Resource.DRDW_Chosen,'docsupport/init.js')}" />-->
    </head>  
    <body>                    
        <div class='df_drdw content'>
            <!--Using QFORs Index component for the Top Header -->
            <c:DRDW_IndexNew />
            <div class="df_drdw-createstudy vertical-compress columns-compressed">
                
                <header>
                    
                    <div class="container">
                        <div class="row">
                            <div class="col-xs-12">
                                <apex:outputpanel rendered="{!IF(($CurrentPage.parameters.id == NULL)&&!isnewIdea,true,false)}">
                                    <h1><apex:outputLink value="{!IF(isIdea=true,'/apex/DRDW_CreateIdea?id='+oRisk.Related_To__c+'&isEdit=false&section=#Project_Risks',IF(isGeneration=true,'/apex/DRDW_CreateGeneration?id='+oRisk.Related_To__c+'&isEdit=false&section=#Project_Risks',IF(isnewIdea=true,'/apex/DRDW_CreateNewIdea?id='+oRisk.Related_To__c+'&isEdit=false&section=#Project_Risks','/apex/DRDW_CreateAssessment?id='+oRisk.Related_To__c+'&isEdit=false&section=#Project_Risks')))}">{!projectName}</apex:outputLink> : Create Risk</h1>
                                    <p></p>
                                </apex:outputpanel>   
                                <apex:outputpanel rendered="{!IF(($CurrentPage.parameters.id != NULL)&&!isnewIdea,true,false)}">
                                    <h1>Risk for <apex:outputLink value="{!IF(isIdea=true,'/apex/DRDW_CreateIdea?id='+oRisk.Related_To__c+'&isEdit=false&section=#Project_Risks',IF(isGeneration=true,'/apex/DRDW_CreateGeneration?id='+oRisk.Related_To__c+'&isEdit=false&section=#Project_Risks',IF(isnewIdea=true,'/apex/DRDW_CreatenewIdea?id='+oRisk.Related_To__c+'&isEdit=false&section=#Project_Risks','/apex/DRDW_CreateAssessment?id='+oRisk.Related_To__c+'&isEdit=false&section=#Project_Risks')))}">{!projectName}</apex:outputLink> 
                                    </h1>
                                    <p></p>
                                </apex:outputpanel>  
                                <apex:outputpanel rendered="{!IF(($CurrentPage.parameters.id == NULL)&&isnewIdea,true,false)}">
                                    <h1><apex:outputLink value="{!IF(isIdea=true,'/apex/DRDW_CreateIdea?id='+oRisk.Related_To__c+'&isEdit=false&section=#Project_Risks',IF(isGeneration=true,'/apex/DRDW_CreateGeneration?id='+oRisk.Related_To__c+'&isEdit=false&section=#Project_Risks',IF(isnewIdea=true,'/apex/DRDW_CreateNewIdea?id='+oRisk.Related_To__c+'&isEdit=false&section=#Project_Risks','/apex/DRDW_CreateAssessment?id='+oRisk.Related_To__c+'&isEdit=false&section=#Project_Risks')))}">{!projectName}</apex:outputLink> : Create Gap</h1>
                                    <p></p>
                                </apex:outputpanel>  
                                <apex:outputpanel rendered="{!IF(($CurrentPage.parameters.id != NULL)&&isnewIdea,true,false)}">
                                    <h1>Gap for <apex:outputLink value="{!IF(isIdea=true,'/apex/DRDW_CreateIdea?id='+oRisk.Related_To__c+'&isEdit=false&section=#Project_Risks',IF(isGeneration=true,'/apex/DRDW_CreateGeneration?id='+oRisk.Related_To__c+'&isEdit=false&section=#Project_Risks',IF(isnewIdea=true,'/apex/DRDW_CreatenewIdea?id='+oRisk.Related_To__c+'&isEdit=false&section=#Project_Risks','/apex/DRDW_CreateAssessment?id='+oRisk.Related_To__c+'&isEdit=false&section=#Project_Risks')))}">{!projectName}</apex:outputLink> 
                                    </h1>
                                    <p></p>
                                </apex:outputpanel>                                                            
                            </div>
                        </div>
                    </div>
                    
                </header>
                <main class="container">
                <apex:pageMessages />
                <!-- ----------------------------- Create new Idea form starts------------------------- -->
                <apex:outputpanel rendered="{!IF($CurrentPage.parameters.id == NULL,true,false)}">
                
                        <apex:form Styleclass="create-study-form accordion">
                            <apex:pageMessages />      
                            
                         <section class="row submit with-top-margin">
                                <div class="col-xs-12">
                                     <input type="button" class="btn button submit-form-button" value="Save"/>
                                    <apex:actionFunction Name="doSubmit" action="{!SaveRisk}"/>
                                    <!--apex:commandButton styleClass="btn button" value="Save" action="{!SaveRisk}" /-->
                                    <apex:outputPanel rendered="{!If(isIdea==true,'true','false')}">
                                        <apex:outputLink styleClass="btn button" value="/apex/DRDW_CreateIdea?id={!projectId}&isEdit=false&section=#Project_Risks">Cancel</apex:outputLink>
                                    </apex:outputPanel>
                                    <apex:outputPanel rendered="{!if(isGeneration==true,'true','false')}">
                                        <apex:outputLink styleClass="btn button" value="/apex/DRDW_CreateGeneration?id={!projectId}&isEdit=false&section=#Project_Risks">Cancel</apex:outputLink>
                                    </apex:outputPanel>
                                    <apex:outputPanel rendered="{!if(isAssessment==true,'true','false')}">
                                        <apex:outputLink styleClass="btn button" value="/apex/DRDW_CreateAssessment?id={!projectId}&isEdit=false&section=#Project_Risks">Cancel</apex:outputLink>
                                    </apex:outputPanel>
                                    <apex:outputPanel rendered="{!if(isnewIdea==true,'true','false')}">
                                        <apex:outputLink styleClass="btn button" value="/apex/DRDW_CreateNewIdea?id={!projectId}&isEdit=false&section=#Project_Risks">Cancel</apex:outputLink>
                                    </apex:outputPanel>
                                </div>
                            </section>
                            
                         <apex:outputpanel rendered="{!isIdea || isGeneration}">        
                         <section class="row">
                                <div class="col-xs-12">
                               
                                    <h2 class="accordion-parent">Details
                                        <span class="icon-large-caret pull-right down"></span></h2>
                               
                                
                                    <fieldset class="accordion-child">
                                        
                                        <div class="row">
                                            <div class="col-xs-3 label-wrapper left-aligned">
                                                <label>{!$ObjectType.DRDW_Risk__c.Fields.Related_To__c.Label}</label>
                                                <apex:outputpanel rendered="{!IF($ObjectType.DRDW_Risk__c.Fields.Related_To__c.inlinehelptext != NULL,true,false)}">
                                                    <div class="helpOrb tooltip">
                                                        <span>{!$ObjectType.DRDW_Risk__c.Fields.Related_To__c.inlinehelptext}</span>
                                                    </div>          
                                                </apex:outputpanel>      
                                            </div>
                                            <div class="col-xs-3 field-wrapper left-aligned">
                                            <apex:outputLink value="/apex/DRDW_CreateIdea?id={!oRisk.Related_To__c}&isEdit=false" rendered="{!isIdea}">{!projectName}</apex:outputLink>
                                             <apex:outputLink value="/apex/DRDW_CreateGeneration?id={!oRisk.Related_To__c}&isEdit=false" rendered="{!isGeneration}">{!projectName}</apex:outputLink>
                                            </div>
                                            <div class="col-xs-3 label-wrapper left-aligned">
                                                <label>{!$ObjectType.DRDW_Risk__c.Fields.Description__c.Label}</label><span class="required-label"> *</span>
                                                <apex:outputpanel rendered="{!IF($ObjectType.DRDW_Risk__c.Fields.Description__c.inlinehelptext != NULL,true,false)}">
                                                    <div class="helpOrb tooltip">
                                                        <span>{!$ObjectType.DRDW_Risk__c.Fields.Description__c.inlinehelptext}</span>
                                                    </div>          
