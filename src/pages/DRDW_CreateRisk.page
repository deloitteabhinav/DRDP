                                            </div>
                                            <div class="col-xs-3 field-wrapper left-aligned">
                                                <apex:inputField value="{!oRisk.Title__c}" styleclass="required"></apex:inputField>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-xs-3 label-wrapper left-aligned">
                                                <label>{!$ObjectType.DRDW_Risk__c.Fields.Description_Gap__c.Label}</label><span class="required-label"> *</span>
                                                <apex:outputpanel rendered="{!IF($ObjectType.DRDW_Risk__c.Fields.Description_Gap__c.inlinehelptext != NULL,true,false)}">
