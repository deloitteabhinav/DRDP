                                                <apex:inputField value="{!oRisk.Description__c}" styleclass="required"></apex:inputField>
                                            </div>
                                        </div>
     
                                    </fieldset>
                                </div>
                            </section>
                          </apex:outputPanel>
                         <apex:outputpanel rendered="{!isnewIdea}">        
