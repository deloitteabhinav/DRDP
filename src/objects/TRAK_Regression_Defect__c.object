    <validationRules>
        <fullName>Make_RootCause_and_Resolution_Mandatory</fullName>
        <active>true</active>
        <description>Please update the Root Cause and Resolution before updating the Regression Defect Status to Complete.</description>
        <errorConditionFormula>AND(OR(ISPICKVAL(Status__c, &apos;Passed Test&apos;), ISPICKVAL(Status__c, &apos;Passed Test PreProd&apos;),ISPICKVAL(Status__c, &apos;Production - Released&apos;)),OR((Root_Cause__c == null), (Resolution__c == null)) )</errorConditionFormula>
        <errorMessage>Please update the Root Cause and Resolution before updating the Regression Defect Status to Complete.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reg_Defect_should_be_Raised_on_Creation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW(),NOT(ISPICKVAL(Status__c,&apos;Raised&apos;)))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>A Regresion Defect can only be created with Raised status.</errorMessage>
    </validationRules>
