    <validationRules>
        <fullName>Defect_Cancellation_Reason_Check</fullName>
        <active>true</active>
        <description>Please enter a Cancellation Reason for Cancelled Status. Please remove the Cancellation Reason if the status is not Cancelled.</description>
        <errorConditionFormula>OR(AND( ISPICKVAL( Status__c, &quot;Cancelled&quot;) , ISBLANK( Closure_Reason__c ) ),AND(NOT(ISPICKVAL( Status__c, &quot;Cancelled&quot;)),NOT(ISBLANK( Closure_Reason__c ))))</errorConditionFormula>
        <errorDisplayField>Closure_Reason__c</errorDisplayField>
        <errorMessage>Please enter a Cancellation Reason for Cancelled Status. Please remove the Cancellation Reason if the status is not Cancelled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Defect_should_be_Raised_on_Creation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW(),NOT(ISPICKVAL(Status__c,&apos;Raised&apos;)))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>A Defect can only be created with Raised status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Developer_cant_be_same_as_Tester</fullName>
        <active>true</active>
        <errorConditionFormula>AND((NOT(ISBLANK(Defect_Developer__c))),(NOT(ISBLANK(Tester__c))),(Defect_Developer__c  = Tester__c))</errorConditionFormula>
        <errorDisplayField>Tester__c</errorDisplayField>
        <errorMessage>The Developer cannot be the same as Tester.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Duplicate_Reference_cannot_refer_same_de</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Duplicate_Reference__c != null,Name != null,CONTAINS(Duplicate_Reference__c, Name))</errorConditionFormula>
        <errorMessage>The Duplicate Reference cannot Refer to the same defect.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Make_Root_Cause_and_Resolution_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISPICKVAL(Status__c, &apos;Passed Complete Test&apos;), ISPICKVAL(Status__c, &apos;Passed Test PreProd&apos;),ISPICKVAL(Status__c, &apos;Production - Released&apos;)),OR((Root_Cause__c == null), (Resolution__c == null))  )</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Please update the Root Cause and Resolution before updating the Defect Status to Complete.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_Creator_can_close_the_Defect</fullName>
        <active>true</active>
        <description>Rule to ensure that the Creator is the only person who can close the defect.</description>
        <errorConditionFormula>AND( Own_Closure__c = true,  CreatedById  &lt;&gt;  $User.Id ,  OR( ISPICKVAL(Status__c ,&apos;Closed - Complete&apos; ), ISPICKVAL(Status__c , &apos;Closed - Rejected&apos;), ISPICKVAL(Status__c , &apos;Closed - Consolidated&apos;), ISPICKVAL(Status__c, &apos;Closed - Cannot Replicate&apos;)   ))</errorConditionFormula>
        <errorMessage>Sorry, only the Creator can close this defect.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>check_for_Comments_when_Status_is_Closed</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR( ISPICKVAL(Status__c,&quot;Closed - Rejected&quot;), ISPICKVAL(Status__c,&quot;Closed - Consolidated&quot;)  ) , ISBLANK(Closure_Reason__c) )</errorConditionFormula>
        <errorMessage>Please populate the Closure Reason field to indicate why defect has been Rejected or Consolidated.</errorMessage>
    </validationRules>
</CustomObject>
