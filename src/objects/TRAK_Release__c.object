    <validationRules>
        <fullName>Approvers_cannot_be_same</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISCHANGED(Number_of_User_Stories__c)),(AND(ISNEW(),Number_of_User_Stories__c &gt; 0)),OR((AND((NOT(ISBLANK(Business_Analyst__c))),(NOT(ISBLANK(System_Custodian__c))),(Business_Analyst__c = System_Custodian__c))),(AND((NOT(ISBLANK(Business_Analyst__c))),(NOT(ISBLANK(Product_Owner__c))),(Business_Analyst__c = Product_Owner__c))),(AND((NOT(ISBLANK(System_Custodian__c))),(NOT(ISBLANK(Product_Owner__c))),(System_Custodian__c = Product_Owner__c)))))</errorConditionFormula>
        <errorDisplayField>Business_Analyst__c</errorDisplayField>
        <errorMessage>The Business Analyst, Product Owner and System Custodian have to be different users.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Atleast_1_Environment_Release</fullName>
        <active>false</active>
        <errorConditionFormula>AND(( ISPICKVAL( Approval_Status__c , &apos;Production - Released&apos;) ),( Number_of_Environments__c = 0))</errorConditionFormula>
        <errorDisplayField>Approval_Status__c</errorDisplayField>
        <errorMessage>Please associate this release to an environment before updating the status to &apos;Production - Released&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Atleast_1_User_Story_or_Defect</fullName>
        <active>true</active>
        <errorConditionFormula>AND((Number_of_Defects__c = 0 ), ( Number_of_User_Stories__c = 0),  ISPICKVAL( Approval_Status__c , &apos;Ready for Deployment&apos;)   )</errorConditionFormula>
        <errorDisplayField>Approval_Status__c</errorDisplayField>
        <errorMessage>Please associate at least 1 User Story or Defect before moving this release to &apos;Ready for Deployment&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Can_Only_Create_Release_in_Draft_Status</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW(), NOT(ISPICKVAL(Approval_Status__c, &apos;Draft&apos;)))</errorConditionFormula>
        <errorDisplayField>Approval_Status__c</errorDisplayField>
        <errorMessage>A Release record can only be created in &apos;Draft&apos; status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Manually_Update_Release_Name</fullName>
        <active>true</active>
        <description>Cannot Manually Update Release Name</description>
        <errorConditionFormula>AND(ISCHANGED(Name), (Override_Release_Name__c = false), NOT(ISNEW()))</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>You cannot update the release name. Please check the &apos;Override Release Name&apos; checkbox.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Update_Release_Name</fullName>
        <active>true</active>
        <description>TRAK Cannot Update Release Name</description>
        <errorConditionFormula>AND(ISCHANGED(Name),  (Override_Release_Name__c = false))</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>You cannot change the release name once the release record is created. Please check the override checkbox for updating the release name.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_change_Tag_Number</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED( Tag__c ), OR(ISPICKVAL(Approval_Status__c, &apos;Pending Deployment Approval&apos;), ISPICKVAL(Approval_Status__c, &apos;Deployment Approved&apos;), ISPICKVAL(Approval_Status__c, &apos;Deployment Rejected&apos;), ISPICKVAL(Approval_Status__c, &apos;Production - Released&apos;)))</errorConditionFormula>
        <errorDisplayField>Tag__c</errorDisplayField>
        <errorMessage>Please update the Release Status to &apos;Ready for Deployment&apos; for updating the Tag number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_update_Override_Flag</fullName>
        <active>true</active>
        <description>Cannot Update Override Flag</description>
        <errorConditionFormula>AND((PRIORVALUE(Override_Release_Name__c) == true), (Override_Release_Name__c = false))</errorConditionFormula>
        <errorDisplayField>Override_Release_Name__c</errorDisplayField>
        <errorMessage>You cannot update the Override Release Name flag once it is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Monthly_Release</fullName>
        <active>false</active>
        <errorConditionFormula>ISPICKVAL( Release_Type__c , &apos;Monthly&apos;)</errorConditionFormula>
        <errorDisplayField>Release_Type__c</errorDisplayField>
        <errorMessage>Currently Monthly Releases are not supported.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Production_Release_Date_cant_be_in_past</fullName>
        <active>true</active>
        <errorConditionFormula>AND((Release_Date__c &lt; today()),OR(ISNEW(),ISCHANGED(Release_Date__c)))</errorConditionFormula>
        <errorDisplayField>Approval_Status__c</errorDisplayField>
        <errorMessage>Production Release Date should be a date in the future.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Release_Process_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Approval_Status__c, &apos;Ready for Deployment&apos;),  OR(ISBLANK(Branch_Name__c),ISBLANK( Repository_Name__c ),ISBLANK( Tag__c ),ISBLANK( Commit_ID__c )))</errorConditionFormula>
        <errorDisplayField>Approval_Status__c</errorDisplayField>
        <errorMessage>Branch Name, Commit ID, Tag Number and Repository Name are mandatory before submitting the release for approval.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TRAK_Rejected_Reason_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>ISNEW() &amp;&amp; Rejected__c  == FALSE &amp;&amp; NOT(ISBLANK( Rejected_Reason__c ))</errorConditionFormula>
        <errorDisplayField>Rejected_Reason__c</errorDisplayField>
        <errorMessage>The rejected field needs to be checked to provide a rejection reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Tag_Name_Format</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(Contains( Tag__c , &apos;TAG_&apos;)))</errorConditionFormula>
        <errorDisplayField>Tag__c</errorDisplayField>
        <errorMessage>Please enter a valid Tag Number.</errorMessage>
    </validationRules>
