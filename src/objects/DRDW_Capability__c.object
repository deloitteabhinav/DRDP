    <fields>
        <fullName>Target_End_Date__c</fullName>
        <externalId>false</externalId>
        <formula>IF( CONTAINS(TEXT(Duration__c),&apos;Week&apos;),
( 7 * VALUE(LEFT(TEXT(Duration__c),1)) + Actual_Start_Date__c)
,DATE(
    YEAR( Actual_Start_Date__c ) +
    FLOOR((VALUE(LEFT(TEXT(Duration__c),1)) + MONTH(Actual_Start_Date__c)) / 12) -
    IF (MOD(MONTH(Actual_Start_Date__c) + VALUE(LEFT(TEXT(Duration__c),1)), 12) = 0,
        1,
        0),

    MOD((VALUE(LEFT(TEXT(Duration__c),1)) + MONTH(Actual_Start_Date__c) - 1), 12) + 1,

    DAY(Actual_Start_Date__c)
))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Target End Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
